<h1>ADD Admin </h1>
<%= form_with model:@admin do |form| %>
    <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>        
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.text_field :password %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
<br>
